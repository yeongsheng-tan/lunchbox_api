{
  "devDependencies": {
    "@babel/core": "^7.0",
    "@babel/preset-env": "^7.0",
    "@cypress/webpack-preprocessor": "^5.4",
    "@typescript-eslint/eslint-plugin": "^4.5",
    "@typescript-eslint/parser": "^4.5",
    "babel-loader": "^8.0",
    "cypress": "^5.4",
    "eslint": "^7.12",
    "eslint-config-prettier": "^6.14",
    "eslint-plugin-cypress": "^2.11",
    "eslint-plugin-prettier": "^3.1",
    "eslint-plugin-testing-library": "^3.9",
    "npm-run-all": "^4.1",
    "prettier": "^2.1",
    "ts-loader": "^6.2",
    "typescript": "^3.9",
    "webpack": "^4.18"
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "start": "export PHX_PORT=7000 DB_PORT=26257 && mix phx.server",
    "reset_db": "export DB_PORT=26257 && mix ecto.drop && mix ecto.setup && mix ecto.migrate",
    "test": "run-p --race start cy:run",
    "format": "prettier --write cypress/**/*.js cypress/**/*.ts'",
    "fix-ts": "eslint --fix 'cypress/**/*.ts'"
  }
}
